public class Carro extends Veiculo {
    private int tempoAceleracao; // em segundos

    public Carro(String marca, String modelo, int ano, int tempoAceleracao) {
        super(marca, modelo, ano);
        this.tempoAceleracao = tempoAceleracao;
    }

    @Override
    public void acelerar() {
        System.out.println(modelo + " " + marca + " est√° acelerando de 0 a 100 km/h em " + tempoAceleracao + " segundos.");
    }
}